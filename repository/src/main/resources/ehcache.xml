<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<!-- 集群配置 -->
	<cacheManagerPeerProviderFactory
		class="net.sf.ehcache.distribution.RMICacheManagerPeerProviderFactory"
		properties="peerDiscovery=manual,  
                        rmiUrls=//localhost:40001/indexSecondProductCategoryCache|
                        //localhost:40001/productCategoryCache|
                        //localhost:40001/secondProductCategoriesCache
                        "
		propertySeparator="," />
	<cacheManagerPeerListenerFactory
		class="net.sf.ehcache.distribution.RMICacheManagerPeerListenerFactory"
		properties="hostName=localhost, port=40002, socketTimeoutMillis=2000" />
	<!-- productCategory -->
	<cache name="indexSecondProductCategoryCache"
		maxEntriesLocalHeap="5000" maxEntriesLocalDisk="10000" eternal="false"
		diskSpoolBufferSizeMB="20" timeToIdleSeconds="200" timeToLiveSeconds="500"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
	<cache name="productCategoryCache" maxEntriesLocalHeap="5000"
		maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="20"
		timeToIdleSeconds="200" timeToLiveSeconds="500"
